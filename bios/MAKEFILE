CPM3		TESTLDR.COM CPMLDR3.COM TESTZPM3.COM TESTDRI3.COM


TESTZPM3.COM	BOOTSYS.BIN ZPM3.SYS
		PIP TESTZPM3.COM=BOOTSYS.BIN,ZPM3.SYS

ZPM3.SYS	BNKBIOS3.SPR BNKBDOS3.SPZ RESBDOS3.SPZ GENCPM.ZPR
		PIP BNKBDOS3.SPR[V]=BNKBDOS3.SPZ
		PIP RESBDOS3.SPR[V]=RESBDOS3.SPZ
		ERA GENCPM.DAT
		PIP GENCPM.DAT=GENCPM.ZPR
		GENCPM3 AUTO DISPLAY
		ERA TEST.SYS
		REN TEST.SYS=CPM3.SYS
		PIP ZPM3.SYS=TEST.SYS


TESTDRI3.COM	BOOTSYS.BIN DRI3.SYS
		PIP TESTDRI3.COM=BOOTSYS.BIN,DRI3.SYS

DRI3.SYS	BNKBIOS3.SPR BNKBDOS3.SPD RESBDOS3.SPD GENCPM.DRI
		PIP BNKBDOS3.SPR[V]=BNKBDOS3.SPD
		PIP RESBDOS3.SPR[V]=RESBDOS3.SPD
		ERA GENCPM.DAT
		PIP GENCPM.DAT=GENCPM.DRI
		GENCPM3 AUTO DISPLAY
		PIP DRI3.SYS=CPM3.SYS


CPMLDR3.COM	CPMLDR3.REL LDRBIOS.REL
		LINK CPMLDR3=CPMLDR3,LDRBIOS

TESTLDR.COM	TESTLDR.REL LDRBIOS.REL
		LINK TESTLDR=TESTLDR,LDRBIOS

CPMLDR3.REL	CPMLDR3.MAC
		ZSM4 CPMLDR3,CPMLDR3.LIS=CPMLDR3

TESTLDR.REL	CPMLDR3.MAC
		ZSM4 TESTLDR,TESTLDR.LIS=CPMLDR3/DTEST=1

LDRBIOS.REL	LDRBIOS.MAC
		ZSM4 LDRBIOS,LDRBIOS.LIS=LDRBIOS


BNKBIOS3.SPR	BIOSKRNL.REL BOOT.REL CHARIO.REL MOVE.REL SCB.REL VDISK.REL
		LINK BNKBIOS3[B]=BIOSKRNL,BOOT,CHARIO,MOVE,SCB,VDISK

BIOSKRNL.REL	BIOSKRNL.MAC
		ZSM4 BIOSKRNL,BIOSKRNL.LIS=BIOSKRNL

BOOT.REL	BOOT.MAC
		ZSM4 BOOT,BOOT.LIS=BOOT/DZPM3=1

CHARIO.REL	CHARIO.MAC
		ZSM4 CHARIO,CHARIO.LIS=CHARIO

MOVE.REL	MOVE.MAC
		ZSM4 MOVE,MOVE.LIS=MOVE

SCB.REL		SCB.MAC
		ZSM4 SCB,SCB.LIS=SCB

VDISK.REL	VDISK.MAC
		ZSM4 VDISK,VDISK.LIS=VDISK

